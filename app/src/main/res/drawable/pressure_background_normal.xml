<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <gradient
                android:type="linear"
                android:angle="0"
                android:startColor="#A1FFFFFF"
                android:centerColor="#A178EC4F"
                android:endColor="#FDFDFD" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="#A3A69B9B" />
            <solid android:color="#00FFFFFF" />

        </shape>
    </item>

    <item
        android:top="1dp"
        android:bottom="1dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="#FFDDDDDD" />
            <solid android:color="#00000000" />
        </shape>
    </item>
</layer-list>